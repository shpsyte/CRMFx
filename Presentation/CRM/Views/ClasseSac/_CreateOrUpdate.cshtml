@model PS_Classe_Sac

@Html.AntiForgeryToken()
@Html.HiddenFor(model => model.cod_classe)

<fieldset>
    <section>
        <label class="label">Classe de Sac</label>
        <label class="input">
            <i class="icon-prepend fa fa-user"></i>
            @Html.TextBoxFor(m => m.des_nome, new { @class = "form-control" })
            @Html.ValidationMessageFor(model => model.des_nome)
        </label>
    </section>


</fieldset>
