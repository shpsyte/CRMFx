@model Segmentos

@Html.AntiForgeryToken()
@Html.HiddenFor(model => model.segmentoid)

<fieldset>
    <section>
        <label class="label">Nome</label>
        <label class="input">
            <i class="icon-prepend fa fa-user"></i>
            @Html.TextBoxFor(m => m.des_segmento, new { @class = "form-control" })
            @Html.ValidationMessageFor(model => model.des_segmento)
        </label>
    </section>



</fieldset>
